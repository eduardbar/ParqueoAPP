{
  "name": "parqueo-backend",
  "version": "1.0.0",
  "description": "Backend API for Parqueo App",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "dev:backend": "nodemon src/index.ts",
    "build": "npm run prisma:generate && tsc",
    "start": "npm run prisma:generate && npm run prisma:migrate:deploy && node dist/index.js",
    "test:watch": "jest --watch",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "jwt"
  ],
  "author": "ParqueoApp Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.9",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "redis": "^4.6.7",
    "socket.io": "^4.8.1",
    "stripe": "^18.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "prisma": "^5.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^2.0.12",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1"
  }
}
